{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "resourceClass": "medium"
      }
    },
    "production": {
      "distribution": "store",
      "prebuildCommand": "bash eas-build-pre-install.sh",
      "ios": {
        "resourceClass": "medium",
        "autoIncrement": true
      },
      "android": {
        "resourceClass": "medium",
        "gradleCommand": ":app:assembleRelease",
        "applicationArchivePath": "android/app/build/outputs/apk/release/app-release.apk",
        "autoIncrement": true
      }
    },
    "release": {
      "extends": "production",
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
