# Firebase Configuration
# Get these from Firebase Console > Project Settings
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id

# Google Maps API Keys
# Get these from Google Cloud Console > APIs & Services > Credentials
# Enable: Maps SDK for Android/iOS, Places API, Geocoding API
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY_IOS=your_ios_google_maps_api_key_here
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY_ANDROID=your_android_google_maps_api_key_here

# API Configuration
# Your backend API URL
EXPO_PUBLIC_API_URL=https://your-api-url.com/api
